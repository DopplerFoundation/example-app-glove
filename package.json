{
  "name": "Conversational NLP",
  "slug": "conversational-nlp",
  "keywords": "conversational nlp",
  "description": "Discover the words that are most frequently used in conversation with each other.",
  "main": "app/main.py",
  "repository": "https://github.com/DopplerFoundation/example-app-conversational-nlp",
  "input_scheme": {
    "words": "object",
    "limit": "number"
  },
  "output_scheme": {
    "words": "object"
  },
  "input_example": {
    "words": ["blue", "twitter", "fkdsjfsa"],
    "limit": 5
  },
  "output_example": {
    "words": {
      "blue": [
        {"label": "yellow", "score": 0.96},
        {"label": "red", "score": 0.95},
        {"label": "green", "score": 0.95},
        {"label": "purple", "score": 0.95},
        {"label": "black", "score": 0.94}
      ],
      "twitter": [
        {"label": "facebook", "score": 0.95},
        {"label": "tweet", "score": 0.94},
        {"label": "fb", "score": 0.93},
        {"label": "instagram", "score": 0.91},
        {"label": "chat", "score": 0.9}
      ]
    }
  },
  "license": "Apache License"
}
